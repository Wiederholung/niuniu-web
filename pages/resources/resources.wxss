/* pages/resources/resources.wxss */
.swiper-wrap swiper{
    width:100%;
    height: 240rpx;
  }
  .swiper-wrap swiper image{
    width: 100%;
  }
  
  .icons-wrap{
    display: flex;
    margin:30rpx 0;
  }
  .icons-wrap navigator{
    flex: 1;
    width: 20%;
    display: flexbox;
    text-align: center;
    vertical-align: center;
  }
  .icons-wrap navigator image{
    width: 70%;
  }
  .icons-wrap navigator text{
    font-size: 28rpx;
    color: rgb(27, 26, 26);
  }
  
  .mode-wrapper{
    padding:20rpx;
    border-top: solid 2rpx #eee;
  }
  .mode-wrapper .mode-title{
    font-size: 30rpx;
    color: rgb(63, 61, 61);
    display: flex;
    justify-content:space-between;
  }
  
  .floor-wrap .floor-list{
    float:left;
    width: 49%;
    text-align: center;
    margin-bottom: 30rpx;
    overflow: hidden;
    height: 250rpx;
  }
  .floor-wrap .floor-list image{
    width: 80%;
  }
  .tips-wrapper .tips-content{
    line-height: 50rpx;
    color: rgb(43, 40, 40);
    font-size: 28rpx;
    padding:20rpx;
  }